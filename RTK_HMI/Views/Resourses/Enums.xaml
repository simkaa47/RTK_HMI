<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:col="clr-namespace:System.Collections;assembly=System.Runtime.Extensions"
                    xmlns:sys="clr-namespace:System;assembly=System.Runtime">
    <col:ArrayList x:Key="PortSpeeds">
        <sys:Int32>2400</sys:Int32>
        <sys:Int32>4800</sys:Int32>
        <sys:Int32>9600</sys:Int32>
        <sys:Int32>19200</sys:Int32>
        <sys:Int32>38400</sys:Int32>
        <sys:Int32>115200</sys:Int32>
    </col:ArrayList>
    <col:ArrayList x:Key="ConnWays">
        <sys:String>Modbus RTU</sys:String>
        <sys:String>Modbus UDP</sys:String>        
    </col:ArrayList>
</ResourceDictionary>